<html>
<head>
	<title>Profile</title>
	<link rel="stylesheet" type="text/css" href="css/bootstrap.css">
	<link rel="stylesheet" type="text/css" href="profile.css">
</head>
<body>
<div class="row">

<% var ab=res[0]._id %>
<b><%= "  welcome "+res[0].Name%></b>
<br><br>
<% for(var a=0;a < res1.length;a++) {
var b=res1[a]._id %> 
 <%  if(ab!=b){%>
<div class="col-sm-6 col-md-1" id="r"></div>
  <div class="col-sm-6 col-md-2" id="r">
    <div class="thumbnail">
      <img alt="100%x200" data-src="holder.js/100%x200" src="data:image/svg+xml;base64," data-holder-rendered="true" style="height: 200px; width: 100%; display: block;">
      
      <div class="caption">
        <h3><%= res1[a].Name%></h3>
        <p><%= " Email:"+res1[a].Email%><br>
        <%=" User:"+res1[a].UserName%> </p>
     
    </div>
  </div> 
  </div>
<% } %>
<% } %>
</div>
</body>
</html>